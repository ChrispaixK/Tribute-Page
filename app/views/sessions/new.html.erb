<h1>Login to xblog </h1>
<%= form_with(scope: :session,url: login_path, local: true) do |f| %>
  <%= f.label :email %>
  <%= f.email_field :email, placeholder: 'Enter email' %>
  <br>

  <%= f.label :password %>
  <%= f.password_field :password, placeholder: 'Enter password' %>
  <p>
    <%= f.submit "Log in" %>
  </p>
<% end %>